{
    "manifest_version": 2,
    "name": "Focus",
    "version": "1.0",
    "description": "To help one focus and look at less YouTube",
    "content_scripts": [
        {
            "matches": ["https://*.google.com/*","https://*.youtube.com/*", "https://www.facebook.com/*", "https://www.tiktok.com/*"],  
            "js": ["./main.js"]
        }
    ]
}